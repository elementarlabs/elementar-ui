{
  "name": "elementar-ui",
  "description": "Elementar Admin - Free and open-source Angular UI components and Admin panel based on Material 3 and Tailwind",
  "version": "1.0.0",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/elementarlabs/elementar-ui/issues"
  },
  "homepage": "https://elementarui.com",
  "keywords": [
    "elementar admin",
    "angular admin",
    "admin panel",
    "angular dashboard",
    "angular admin panel",
    "angular material components",
    "angular admin template",
    "angular ui"
  ],
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "build": "ng build",
    "build:components:prod": "ng build components --configuration production",
    "build:prod": "ng build --configuration production",
    "changelog": "auto-changelog -p && git add CHANGELOG.md",
    "prepare": "husky"
  },
  "private": true,
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
